    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "X = np.zeros(20)\n",
    "i=-5\n",
    "j=0\n",
    "while i<=5 and j<20:\n",
    "    X[j] = i\n",
    "    i+= .5\n",
    "    j+= 1"
   ]
  },
  {
   "cell_type": "code",
